{
  "name": "xiag/rql-command",
  "keywords": ["rest", "rql", "resource", "query", "language", "parser", "command", "cli"],
  "license": "GPL-2.0",
  "autoload": {
    "psr-4": { "Xiag\\Rql\\Command\\": "src/"}
  },
  "require": {
    "php": ">=5.5",
    "xiag/rql-parser": "~1.0.1",
    "symfony/console": ">=2.6"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~2.0"
  },
  "scripts": {
    "cs": [
      "vendor/bin/phpcs --standard=PSR1 src/",
      "vendor/bin/phpcs --standard=PSR2 src/"
    ]
  },
  "minimum-stability": "stable"
}
